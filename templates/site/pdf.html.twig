

{% extends 'base.html.twig' %}

{% block title %}un devis {% endblock %}

{% block body %}
    <table border="2">
            <tr>
                <th>name</th>
                <th>Description</th>
                <th>price</th>
            </tr>
            {%  for sites  in sites %}
                <tr>
                    <td>{{ sites.nom }}</td>
                    <td>{{ sites.description }}</td>
                    <td>{{ sites.prix }} TND </td>
                </tr>
            {% endfor %}
    </table>
{% endblock %}
